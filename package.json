{
  "name": "JL",
  "version": "1.0.0",
  "main": "index.js",
  "dependencies": {
    "glob-run": "^0.1.6",
    "jshint": "^2.9.5",
    "onchange": "^3.2.1",
    "parallelshell": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "nu-html-checker": "^0.1.0",
    "browser-sync": "^2.18.13",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "test": "npm run test:html && npm run test:js",
    "test:js": "glob-run jshint *.js",
    "test:html": "glob-run nu-html-checker *.html",
    "build": "npm run build:sass && npm run build:autoprefixer && npm run build:clean && npm run test ",
    "build:clean": "rimraf .sass-cache",
    "build:sass": "sass --sourcemap=none --update style.scss:style.css",
    "build:autoprefixer": "autoprefixer-cli style.css",
    "watch": "parallelshell \"npm run watch:sass\" \"npm run watch:autoprefixer\" \"npm run watch:browsersync\"",
    "watch:sass": "sass --watch style.scss:style.css",
    "watch:autoprefixer": "onchange -d 100 style.css -- autoprefixer-cli -o style.prefixed.css style.css",
    "watch:browsersync": "browser-sync --reload-delay 200 start --server --files \"*.css\" \".html\" "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julita45/ID-page.git"
  },
  "bugs": {
